<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_marginEnd="180dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="83dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="616dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.229"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="120dp"
            android:id="@+id/textView" />
        <Constraint
            motion:layout_constraintEnd_toStartOf="@id/textView"
            android:layout_width="30dp"
            android:layout_height="37dp"
            motion:layout_constraintHorizontal_bias="0.2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintDimensionRatio="h,1:1"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp"
            android:id="@+id/buttonB"
            android:layout_marginStart="50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="614dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>